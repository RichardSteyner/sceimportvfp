<apex:page standardController="Project__c" extensions="ProjectDashboardController" showHeader="false" applyBodyTag="false" standardStylesheets="false" applyHtmlTag="false" doctype="html-5.0">
<head>
<style>
    html{line-height:1.15;-ms-text-size-adjust:100%;-webkit-text-size-adjust:100%;}
    article,aside,footer,header,nav,section{display:block}
    h1{font-size:2em;margin:.67em 0}
    figcaption,figure,main{display:block}
    figure{margin:1em 40px}
    hr{box-sizing:content-box;height:0;overflow:visible}
    pre{font-family:monospace,monospace;font-size:1em}
    a{background-color:transparent;-webkit-text-decoration-skip:objects}
    abbr[title]{border-bottom:none;text-decoration:underline;text-decoration:underline dotted}
    b,strong{font-weight:inherit;font-weight:bolder}
    code,kbd,samp{font-family:monospace,monospace;font-size:1em}
    dfn{font-style:italic}
    mark{background-color:#ff0;color:#000}
    small{font-size:80%}
    sub,sup{font-size:75%;line-height:0;position:relative;vertical-align:baseline}
    sub{bottom:-.25em}
    sup{top:-.5em}
    audio,video{display:inline-block}
    audio:not([controls]){display:none;height:0}
    img{border-style:none}
    svg:not(:root){overflow:hidden}
    button,input,optgroup,select,textarea{margin:0}
    button,input{overflow:visible}
    button,select{text-transform:none}
    button,html [type="button"],/* 1 */
    [type="reset"],[type="submit"]{-webkit-appearance:button}
    button::-moz-focus-inner,[type="button"]::-moz-focus-inner,[type="reset"]::-moz-focus-inner,[type="submit"]::-moz-focus-inner{border-style:none;padding:0}
    button:-moz-focusring,[type="button"]:-moz-focusring,[type="reset"]:-moz-focusring,[type="submit"]:-moz-focusring{outline:1px dotted ButtonText}
    legend{box-sizing:border-box;color:inherit;display:table;max-width:100%;padding:0;white-space:normal}
    progress{display:inline-block;vertical-align:baseline}
    textarea{overflow:auto}
    [type="checkbox"],[type="radio"]{box-sizing:border-box;padding:0}
    [type="number"]::-webkit-inner-spin-button,[type="number"]::-webkit-outer-spin-button{height:auto}
    [type="search"]{-webkit-appearance:textfield;outline-offset:-2px}
    [type="search"]::-webkit-search-cancel-button,[type="search"]::-webkit-search-decoration{-webkit-appearance:none}
    ::-webkit-file-upload-button{-webkit-appearance:button;font:inherit}
    details,/* 1 */
    menu{display:block}
    summary{display:list-item}
    canvas{display:inline-block}
    template{display:none}
    [hidden]{display:none}    
    @page { size: 17in 11in; margin:0.5px; font-family:Arial,"Helvetica Neue",Helvetica,sans-serif;}
    body{
        font-family:Arial,"Helvetica Neue",Helvetica,sans-serif;
    }
    .header{
        position:relative;
        width:16.8in;
        background: url({!URLFOR($Resource.ProjectDashboard,'header.png')}) no-repeat;
        //background-size: cover;
        height: 0.5in;
        margin-top: 0.12in;
        /*margin-bottom: 0.1in;*/
    }
    .footer{
        width:16.8in;
        background: url({!URLFOR($Resource.ProjectDashboard,'footer.png')}) no-repeat;
        background-size: cover;
        //width: 16.5in;
        height: 0.5in;
        margin-bottom: 0.15in;
    }
    .header .logo {float: left;}
    .header .name {float: right; font-size: 24px;font-weight: 700; padding-top: 15px; text-transform:uppercase;padding-right:90px;}
    .border{border: 6px solid #E4E4E6;}
    .divLeft{float:left;}
    .divRight{float:left;}
    .content{display: inline-block;width: 16.8in;margin-left: 0.05in;}
    .projectSummary{
        width: 5.75in;
        height: 3.7in;
        //margin-left: 0.25in;
        padding-left: 0.05in;
        padding-right: 0.05in;
        margin-bottom: 0.05in;
        margin-top: 0.05in;
    }
    .underConstruction{
        background: url({!underConstructionUrl}) no-repeat;
        background-size: 100% 100%;
        overflow: hidden;
    }
    .projectTeam{
        width: 5.75in;
        height: 2.6in;
        //margin-left: 0.25in;
        padding-left: 0.05in;
        padding-right: 0.05in;
        margin-bottom: 0.05in;
    }
    .projectGoals{
        width: 5.75in;
        height: 2.73in;
        //margin-left: 0.25in;
        padding-left: 0.05in;
        padding-right: 0.05in;
        margin-bottom: 0.05in;
    }
    .projecTimeline{
        position:relative;
        width:10.5in;
        height:5.3in;
        margin-top: 0.05in;
        margin-left: 0.05in;
        padding-left: 0.05in;
        padding-right: 0.05in;
        margin-bottom: 0.05in;
    }
    .gates{
        position:absolute;
        width:350px;
        height:27px;
        background: url({!URLFOR($Resource.ProjectDashboard,'gates.png')}) no-repeat;
        background-size: 100% 100%;
        overflow: hidden;
        left:50px;
        bottom:-2px;
    }
    .projectUnder{
        display:inline-block;
    }
    .projectStakeholderEnvironment, .projectStakeholderInterestMatrix{
        float:left;
        width: 5.113in;
        height: 3.9in;
        margin-left: 0.05in;
        padding-left: 0.05in;
        padding-right: 0.05in;
        margin-bottom: 0.05in;
    }
    h2.title{ 
        margin-top: 4px;
        margin-bottom: 10px;
        font-size: 20px;
    }
    span.subtitle{
        font-size: 15px;
        font-weight: 600;
    }
    p{
        font-size: 12px;
    }
    thead tr td {background-color:#317CB6;color: #fff;font-size: 16px;}
    .tableIntro thead tr td, .tableIntro2 thead tr td {background-color:#B3CAE3;color: #fff;font-size: 16px;}
    .tableIntro tr td{border:0.5px solid #B3CAE3;}
    .tableStakeHolderInterestMatrix tr td{border:1px solid #929863;}

    .w1 tbody tr td{height:0.5in;}
    .enviromentleft{
        width:79.7%;display: inline-block;margin-right: -2px;border: 0.5px solid #B3CAE3;vertical-align:top;    
    }
    .enviromentleft.h1, .enviromentright.h1{
        height:0.379in;
    }
    .enviromentleft.h2, .enviromentright.h2{
        height:0.26in;
    }
    .enviromentleft.h2>p{
        margin: 0.06in 0;font-size:12px;font-family:Arial,'Helvetica Neue',Helvetica,sans-serif;
    }
    .enviromentright{
        width:18.7%;display: inline-block;margin-left: -2.5px;border: 0.5px solid #B3CAE3;text-align:center;vertical-align:top;    
    }
    @media print {
        .enviromentleft{
            width:79% !important;
        }
        .enviromentright{
            width:19.1% !important;
        }
        .enviromentleft.h1, .enviromentright.h1{
            height:0.369in;
        }
        .enviromentleft.h2, .enviromentright.h2{
            height:0.25in;
        }
    }
    .multiLines tbody tr:nth-child(odd) {
        background-color:#f2f2f2;
    }
    .multiLines tbody tr:nth-child(even) {
        background-color:#fbfbfb;
    }
    .text-left{font-size:14px;padding-left: 0.3in;padding-top: 9px;}
    .developedBy{font-style: italic;font-size:10px;}
    
    /*---TimeLine---*/
    .timeline-wrap {
        position: relative;
        width: 100%;
        height: 30px;
        background: rgb(156, 190, 226);
        /*background: -moz-linear-gradient(left, rgba(49, 124, 182,0.5) 17%, rgba(49, 124, 182,0.7) 58%, rgba(49, 124, 182,1) 100%);
        background: -webkit-linear-gradient(left, rgba(49, 124, 182,0.5) 17%,rgba(49, 124, 182,0.7) 58%,rgba(49, 124, 182,1) 100%);
        background: linear-gradient(to right, rgba(49, 124, 182,0.5) 17%,rgba(49, 124, 182,0.7) 58%,rgba(49, 124, 182,1) 100%);*/
    }
    .arrow-right{
        float: left;
        width: 3px;
        height: 3px;
        margin-top: 240px;
        border-top: 20px solid transparent;
        border-bottom: 25px solid transparent;
        border-left: 35px solid rgb(156, 190, 226);
    }
    .timeline-event {
        position: absolute;
        left: 0; 
        top: 0;
    }
    .timeline-event-node {
        position: absolute;
        display: none;
        top: -3px; 
        left: 0px;
        border-radius: 8px; 
        width: 9px; 
        /*height: 9px;*/
        background: #222;
        cursor: pointer;
        -webkit-transition: width 0.05s ease-out, height 0.05s ease-out, margin 0.05s ease-out, background 0.05s ease-out;
        -moz-transition: width 0.05s ease-out, height 0.05s ease-out, margin 0.05s ease-out, background 0.05s ease-out;
        transition: width 0.05s ease-out, height 0.05s ease-out, margin 0.05s ease-out, background 0.05s ease-out;
    }
    .timeline-event-contents {
        /*display: none;*/
        display: block;
        margin-left: -20px;
    }
    .timeline-event.timeline-selected .timeline-event-node, .timeline-event.timeline-selected .timeline-event-node {
        width: 14px; 
        height: 14px; 
        margin-left: -3px; 
        margin-top: -3px; 
        background: #222;
    }
    .timeline-event-node {
        width: 11px; 
        height: 11px; 
        margin-left: -1px; 
        margin-top: -1px; 
    }
    .timeline-event-contents {
        overflow-x: visible !important;
        overflow-y: visible !important;
        position: absolute;
        /*width: 120px;*/
        height: 30px;
        padding: 0 5px 5px 5px;
        left: 0;
        top: 30px;
        background: rgb(4, 134, 206);
        font: normal 10px/12px 'Lucida Grande', helvetica, tahoma, sans-serif;
        color: #efefef;
    }
    .timeline-event.timeline-bottom .timeline-event-arrow {
        position: absolute;
        /*display: none;*/
        display: block;
        content: "";
        left: 0px;
        top: 24px;
        width: 0;
        height: 0;
        border-left: 6px solid transparent;
        border-right: 6px solid transparent;
        border-bottom: 6px solid #222;
    }
    .timeline-event.timeline-above .timeline-event-contents { 
        top: -80px;
    }
    .timeline-event.timeline-above .timeline-event-arrow {
        top: auto;
        bottom: 32px;
        border-left: 6px solid transparent;
        border-right: 6px solid transparent;
        border-top: 6px solid #222;
        border-bottom: none;
    }
    .timeline-event-contents .timeline-event-content, .timeline-event-contents .timeline-event-link {
        /*display: none;*/
        display: block;
    }
    /*.timeline-event.timeline-selected .timeline-event-content, .timeline-event.timeline-selected .timeline-event-link {
        display: block;
    }*/
    .timeline-event-title {
        white-space: nowrap;
        font-weight: bold;
        margin-top: 3px;
    }
    .timeline-event-title span {
        color: #000;
        display: inline-block;
        margin-right: 10px;
        font-size: 9px;
        font-weight: bold;
    }
    .timeline-event-link {
        display: block;
        text-align: right;
    }
    .timeline-event-link a {
        color: #777;
        text-decoration: none;
        font-size: 10px;
    }
    .timeline-event-link a:visited {
        color: #777;
        text-decoration: none;
    }
    .timeline-event-link a {
        color: #777;
        text-decoration: underline;
    }
    .timeline-large-scale, .timeline-small-scale {
        position: absolute;
        left: 0;
        top: -30px;
        font: bold 10px/18px 'Lucida Grande', helvetica, tahoma, sans-serif;
        color: #222;
    }
    .timeline-dateblock {
        width: 100px;
        text-align: center;
        position: absolute;
        left: 0;
        /*top: 0*/;
        top: 38px;
        margin-left: -50px;
        white-space: nowrap;
    }
    .timeline-dateblock:after {
        display: block;
        content: "";
        position: absolute;
        left: 50px;
        top: -7px;
        width: 1px;
        height: 5px;
        background: #222;
    }
    .timeline-small-scale .timeline-dateblock:before {
        /*top: 25px;*/
        top: 3px;
        height: 4px;
        background: #777;
    }
    .timeline-small-scale {
        color: #000000;
        top: -30px;
    }
    .timeline-event-content img { float: left; margin: 0 10px 10px 0; width: 100px; }
    .timeline-event-content { margin: 5px 0 10px; }    
</style>
</head>
<body>
    <!--<div id="button">-->
    <!--    <a onclick="window.print();">Print</a>-->
    <!--</div>-->
    <div id="dashboard" style="width: 16.8in;">
        <div class="header">
            <div class="logo">
                <img src="{!logoUrl}" style="width: 110px;margin-top: 6px;margin-left: 20px;"/>
                &nbsp;            
            </div>
            <div class="name">
                <apex:outputPanel layout="block" rendered="{!Project__c.Draft__c}" style="position:absolute;width:87px;height:27.2px;top:15px;left:612px;font-size: 24px;font-weight: 700; text-transform:uppercase;">
                    <strong style="color: red;">
                        DRAFT
                    </strong>
                </apex:outputPanel>
                {!Project__c.Name}
            </div>
        </div>
        <div class="content">
            <div class="divLeft">
                <apex:outputPanel layout="block" rendered="{!Project__c.Project_Summary_Display__c}" styleClass="projectSummary border">
                    <h2 class="title">
                        Project Summary as of {!dashboardDateAs}              
                    </h2>
                    <span class="subtitle">
                        <apex:outputPanel rendered="{!if(Project__c.Project_Type__c=='Other', true, false)}">
                            {!Project__c.Other_Project_Type__c}
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!if(Project__c.Project_Type__c=='Other', false, true)}">
                            {!Project__c.Project_Type__c}
                        </apex:outputPanel>
                    </span>
                    <p style="margin: 8px 0;">
                        {!Project__c.Primary_Project_Summary__c}
                    </p>
                    <span class="subtitle">LPA Project Summary</span>
                    <p style="margin: 8px 0;">
                        {!Project__c.Project_Summary__c}
                    </p>
                    <p style="margin: 4px 0;">
                        {!projectSentence}
                    </p>
                </apex:outputPanel>
                <apex:outputPanel layout="block" rendered="{!!Project__c.Project_Summary_Display__c}" styleClass="projectSummary underConstruction border">
                </apex:outputPanel>
                <apex:outputPanel layout="block" rendered="{!Project__c.LPA_Project_Team_Displayed__c}" styleClass="projectTeam border">
                    <table class="multiLines" cellspacing="0" style="margin-top: 0.01in;">
                        <thead>
                            <tr>
                                <td style="width:2.65in;height:0.3in;text-align:center;border-right: 1px solid white;">LPA Project Team<sup>(1)</sup></td>
                                <td style="width:0.8in;height:0.3in;text-align:center;border-left: 1px solid white;border-right: 1px solid white;">% Time<sup>(2)</sup></td>
                                <td style="width:2.2in;height:0.3in;text-align:center;border-left: 1px solid white;">Others Partners - SMEs</td>
                            </tr>
                        </thead>
                        <tbody>
                            <apex:repeat value="{!ProjectTeams}" var="key">
                                <tr style="height:0.2in;">
                                    <td style="border-right: 1px solid white;">
                                        <p style="margin:0;">
                                            <apex:repeat value="{!ProjectTeams[key]}" var="ProjectTeam">
                                                {!ProjectTeam.Contact__r.Name}{!if(isBlank(ProjectTeam.Contact__r.title),'',', '+ProjectTeam.Contact__r.title)}<br/>
                                            </apex:repeat>
                                        </p>
                                    </td>
                                    <td style="text-align:center;border-left: 1px solid white;border-right: 1px solid white;">
                                        <p style="margin:0;">
                                            <apex:repeat value="{!ProjectTeams[key]}" var="ProjectTeam">
                                                <apex:outputPanel rendered="{!IF(contains(dashboardDateAs,'Q1'),true,false)}">{!ProjectTeam.Time_commitment_to_LPA_team_Q1__c}</apex:outputPanel>
                                                <apex:outputPanel rendered="{!IF(contains(dashboardDateAs,'Q2'),true,false)}">{!ProjectTeam.Time_commitment_to_LPA_team_Q2__c}</apex:outputPanel>
                                                <apex:outputPanel rendered="{!IF(contains(dashboardDateAs,'Q3'),true,false)}">{!ProjectTeam.Time_commitment_to_LPA_team_Q3__c}</apex:outputPanel>
                                                <apex:outputPanel rendered="{!IF(contains(dashboardDateAs,'Q4'),true,false)}">{!ProjectTeam.Time_commitment_to_LPA_team_Q4__c}</apex:outputPanel>
                                                <br/>
                                            </apex:repeat>
                                        </p>
                                    </td>
                                    <td style="border-left: 1px solid white;">
                                        <p style="margin:0;">
                                            <apex:repeat value="{!otherParticipants[key]}" var="otherParticipant">
                                                {!otherParticipant.Full_Name__c}{!if(isBlank(otherParticipant.Role__c),'',if(otherParticipant.Full_Name__c==null,otherParticipant.Role__c,', '+otherParticipant.Role__c))}<br/>
                                            </apex:repeat>
                                        </p>
                                    </td>
                                </tr>
                            </apex:repeat>
                        </tbody>
                    </table>
                    <p style="margin: 0;font-size:10px;">
                        (1) Refer to Project Plan for entire list <br/>
                        (2) LPA Project Team percentage time based on current calendar year
                    </p>
                </apex:outputPanel>
                <apex:outputPanel layout="block" rendered="{!!Project__c.LPA_Project_Team_Displayed__c}" styleClass="projectTeam underConstruction border">
                </apex:outputPanel>
                <apex:outputPanel layout="block" rendered="{!Project__c.Goals_Objectives_Displayed__c}" styleClass="projectGoals border">
                    <table width="100%">
                        <thead>
                            <tr>
                                <td style="width:50%;height:0.3in;font-family:Arial,'Helvetica Neue',Helvetica,sans-serif;text-align:center;">Goals</td>
                                <td style="width:50%;height:0.3in;font-family:Arial,'Helvetica Neue',Helvetica,sans-serif;text-align:center;">Objectives</td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="2">
                                    <table width="100%" cellspacing="0" cellpadding="0" border="0">
                                        <apex:repeat value="{!goals}" var="key">
                                            <tr style="height:0.26in;">
                                                <td valign="top" style="width:50%;border: 0.5px solid #B3CAE3;">
                                                    <p style="margin:0;font-family:Arial,'Helvetica Neue',Helvetica,sans-serif;overflow-wrap: break-word;word-wrap: break-word;-ms-word-break: break-all;word-break: break-all;word-break: break-word;-ms-hyphens: auto;-moz-hyphens: auto;-webkit-hyphens: auto;hyphens: auto;">
                                                        {!key}. {!goals[key].Name}
                                                    </p>                                
                                                </td>
                                                <td valign="top" style="width:50%;border: 0.5px solid #B3CAE3;">
                                                    <apex:repeat value="{!objetives[key]}" var="objective">
                                                        <p style="margin:0;font-family:Arial,'Helvetica Neue',Helvetica,sans-serif;overflow-wrap: break-word;word-wrap: break-word;-ms-word-break: break-all;word-break: break-all;word-break: break-word;-ms-hyphens: auto;-moz-hyphens: auto;-webkit-hyphens: auto;hyphens: auto;">
                                                            {!objective.Short_Description__c}<br/>
                                                        </p>                                
                                                    </apex:repeat>
                                                </td>
                                            </tr>
                                        </apex:repeat>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </apex:outputPanel>
                <apex:outputPanel layout="block" rendered="{!!Project__c.Goals_Objectives_Displayed__c}" styleClass="projectGoals underConstruction border">
                </apex:outputPanel>
            </div>
            <div class="divRight">
                <apex:outputPanel layout="block" rendered="{!Project__c.Project_Timeline_Displayed__c}" styleClass="projecTimeline border">
                    <apex:outputPanel layout="block" rendered="{!Project__c.Draft__c}" style="position:absolute;left:55px;top:5px;">
                    </apex:outputPanel>
                    <div style="float: left;display:inline-block;width: 40px;height: 5.3in;background-color: rgba(179, 197, 227, 0.3);">
                        <div style="writing-mode: vertical-lr;width: 40px;height: 2.65in;-webkit-transform: rotate(180deg);-moz-transform: rotate(180deg);-o-transform: rotate(180deg);-ms-transform: rotate(180deg);transform: rotate(180deg);line-height: 50px;text-align: center;">
                            <strong style="font-size:16px; text-transform: capitalize;">
                                <!--MAJOR PROJECTS ORGANIZATION-->{!if(Project__c.Project_Type__c=='Other', Project__c.Other_Project_Type__c, Project__c.Project_Type__c)}
                            </strong>
                        </div>
                        <div style="writing-mode: vertical-lr;width: 40px;height: 2.65in;-webkit-transform: rotate(180deg);-moz-transform: rotate(180deg);-o-transform: rotate(180deg);-ms-transform: rotate(180deg);transform: rotate(180deg);line-height: 50px;text-align: center;">
                            <strong style="font-size:16px">
                                Local Public Affairs
                            </strong>
                        </div>
                    </div>
                    <div id="my-timeline" style="float: left;width: 930px;margin-top: 250px;">
                        <div class="timeline-wrap">
                            <apex:outputPanel layout="block" rendered="{!if(Project__c.Dashboard_Timeline_Start_Date__c!=null && Project__c.Dashboard_Timeline_End_Date__c!=null && Project__c.LPA_Project_Start_Date__c!=null && Project__c.LPA_Est_Completion_Date__c!=null && Project__c.Corporate_Project_Start_Date__c!=null && Project__c.Corporate_Project_Est_Completion_Date__c!=null && Project__c.Dashboard_Timeline_Start_Date__c<=Project__c.LPA_Project_Start_Date__c && Project__c.Dashboard_Timeline_End_Date__c>=Project__c.LPA_Est_Completion_Date__c && Project__c.Dashboard_Timeline_Start_Date__c<=Project__c.Corporate_Project_Start_Date__c && Project__c.Dashboard_Timeline_End_Date__c>=Project__c.Corporate_Project_Est_Completion_Date__c, true, false)}">
                                <div class="timeline-event" data-milestonetype="note-green1" data-startyear="{!timelineStartYear}" data-endyear="{!timelineEndYear}">
                                    <div class="timeline-date">
                                        <apex:outputText value="{0,date,yyyy-MM-dd}">
                                            <apex:param value="{!Project__c.LPA_Project_Start_Date__c}" />
                                        </apex:outputText>
                                    </div>
                                    <div class="timeline-title">&nbsp;</div>
                                    <div class="timeline-thumb"></div>
                                    <div class="timeline-content">
                                        LPA Start
                                    </div>
                                </div>
                                <div class="timeline-event" data-milestonetype="note-green2" data-startyear="{!timelineStartYear}" data-endyear="{!timelineEndYear}">
                                    <div class="timeline-date">
                                        <apex:outputText value="{0,date,yyyy-MM-dd}">
                                            <apex:param value="{!Project__c.Corporate_Project_Start_Date__c}" />
                                        </apex:outputText>
                                    </div>
                                    <div class="timeline-title">&nbsp;</div>
                                    <div class="timeline-thumb"></div>
                                    <div class="timeline-content">
                                        Start
                                    </div>
